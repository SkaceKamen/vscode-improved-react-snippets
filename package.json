{
	"name": "vscode-improved-react-snippets",
	"displayName": "Improved react snippets",
	"description": "Provides basic react snippets, which auto-imports their dependencies",
	"publisher": "skacekachna",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-improved-react-snippets.appendUseState",
				"title": "Import useState"
			},
			{
				"command": "vscode-improved-react-snippets.appendUseMemo",
				"title": "Import useMemo"
			},
			{
				"command": "vscode-improved-react-snippets.appendUseEffect",
				"title": "Import useEffect"
			},
			{
				"command": "vscode-improved-react-snippets.appendUseRef",
				"title": "Import useRef"
			},
			{
				"command": "vscode-improved-react-snippets.appendUseCallback",
				"title": "Import useCallback"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"typescript": "^3.8.3"
	}
}
